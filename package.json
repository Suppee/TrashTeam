{
  "name": "trashteam",
  "version": "1.0.0",
  "description": "Interactive web pages for Odense Renovation",
  "scripts": {
    "css:scss": "node-sass --output-style compressed -o dist src/styles",
    "css:autoprefixer": "postcss -u autoprefixer -r dist/*.css",
    "css:lint": "stylelint src/styles/*.scss  --custom-syntax postcss-scss",
    "build:css": "npm run css:lint && npm run css:scss && npm run css:autoprefixer",
    "watch:css": "onchange \"src/styles\" -- npm run build:css",
    "serve": "browser-sync start --server \"dist\" --files \"dist\"",
    "build:images": "imagemin src/images/**/* --out-dir=dist/images",
    "watch:images": "onchange \"src/images\" -- npm run build:images",
    "build:js": "webpack --mode=production",
    "watch:js": "onchange \"src/js\" -- webpack --mode=development",
    "build:html": "posthtml -c posthtml.json",
    "watch:html": "onchange \"src/views\" -- npm run build:html",
    "watch": "run-p serve watch:*",
    "build": "run-p build:*",
    "format": "npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Suppee/TrashTeam.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Suppee/TrashTeam/issues"
  },
  "homepage": "https://github.com/Suppee/TrashTeam#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "browser-sync": "^2.28.3",
    "eslint": "^8.35.0",
    "eslint-webpack-plugin": "^4.0.0",
    "htmlnano": "^2.0.3",
    "imagemin-cli": "^7.0.0",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^10.1.0",
    "postcss-scss": "^4.0.6",
    "posthtml": "^0.16.6",
    "posthtml-cli": "^0.10.0",
    "posthtml-modules": "^0.9.1",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
